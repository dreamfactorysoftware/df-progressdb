{
  "name":        "dreamfactory/df-progressdb",
  "description": "DreamFactory(tm) Progress Code",
  "homepage":    "https://www.dreamfactory.com/",
  "keywords":    [
    "dreamfactory",
    "progress",
    "connector"
  ],
  "license":     [
    "Apache-2.0"
  ],
  "authors":     [
    {
      "name":  "Yaroslav Mokhurenko",
      "email": "yaroslavm@softwareplanet.com"
    }
  ],
  "support":     {
    "email":  "dspsupport@dreamfactory.com",
    "source": "https://github.com/dreamfactorysoftware/df-progressdb",
    "issues": "https://github.com/dreamfactorysoftware/df-progressdb/issues",
    "wiki":   "https://wiki.dreamfactory.com"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dreamfactorysoftware/df-hadoop"
    }
  ],
  "require":     {
    "dreamfactory/df-core":   "~0.25",
    "dreamfactory/df-system": "~0.5",
    "dreamfactory/df-sqldb": "~0.17",
    "dreamfactory/df-hadoop": "~0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable"
  },
  "autoload":    {
    "psr-4": {
      "DreamFactory\\Core\\Progress\\": "src/"
    }
  },
  "extra":       {
    "branch-alias": {
      "dev-develop": "0.1.x-dev"
    },
    "laravel": {
      "providers": [
        "DreamFactory\\Core\\Progress\\ServiceProvider"
      ]
    }
  }
}
